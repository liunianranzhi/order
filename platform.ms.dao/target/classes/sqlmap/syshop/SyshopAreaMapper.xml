  <select id="selectSyCompanyInfo" resultMap="SyCompanyInfo">
	SELECT 
		a1.area_id, 
		a1.area_name, 
		a1.area_des, 
		a1.area_logo, 
		a2.rec_id, 
		a2.member_id, 
		a2.company_name, 
		a2.company_logo 
	FROM 
		sy_syshop_area a1 
	INNER JOIN 
		sy_syshop_account a2 
	ON 
		a1.area_id = a2.area_id 
	WHERE 
		a1.area_state = 1 
	AND 
		a1.area_sort  <![CDATA[ > ]]> 0 
	AND 
		a2.status = 1 
	ORDER BY 
